 <script>
    function setTheme(themeName) {
      localStorage.setItem('theme', themeName);
      if (document.URL.includes('private')) {
        document.documentElement.className = themeName + " private-page";
      } else {
        document.documentElement.className = themeName + " public-page";
      }
    }

    function toggleTheme() {
      if (localStorage.getItem('theme') === 'theme-dark') {
        setTheme('theme-light');
        var element = document.getElementById("botaothemeID");
        element.classList.remove("fa-sun");
        element.classList.add("fa-moon");
      } else {
        setTheme('theme-dark');
        var element = document.getElementById("botaothemeID");
        element.classList.remove("fa-moon");
        element.classList.add("fa-sun");
      }
    }
    (function () {
      if (localStorage.getItem('theme') === 'theme-dark') {
        setTheme('theme-dark');
        var element = document.getElementById("botaothemeID");
        element.classList.add("fa-sun");
      } else {
        setTheme('theme-light');
        var element = document.getElementById("botaothemeID");
        element.classList.add("fa-moon");
      }
    })();
  </script>
  <script src="/js/jquery.min.js"></script>
  <script src="/js/bootstrap.bundle.min.js"></script>
