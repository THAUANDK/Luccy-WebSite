<script type="text/javascript">
    $('.productSearch').keydown(function() {
        let searchValue = $('.productSearch').val();
        let productList = $('.productList');
        if (searchValue) {
            searchValue = searchValue.toLowerCase();
            for (var i = 0; i < productList.length; i++) {
                let product = productList[i].children[0].children[0].children[1].children[0].children[0].innerHTML.toLowerCase();
                if (product.includes(searchValue)) {
                    $(productList[i]).show()
                } else {
                    $(productList[i]).hide()
                }
            }
        } else {
            for (var i = 0; i < productList.length; i++) {
                $(productList[i]).show()
            }
        }
    })

    $('.productValue2').click(function() {
        let $this = $(this);
        let ids = this.id.split('-');
        let product = products[ids[0]];
        let priceLabel = $('#'+ids[0]+'-price').text(product.prices[ids[1]] + ' Reais')
        for (var i = 0; i < ids[2]+1; i++) {
            if (i == ids[1]) {
                $('#'+ids[0]+'-'+i+'-'+ids[2]).addClass('active');
            } else {
                $('#'+ids[0]+'-'+i+'-'+ids[2]).removeClass('active');
            }
        }
    })

    $('.productValue3').click(function() {
        let $this = $(this);
        let ids = this.id.split('-');
        let product = products[ids[0]];
        let priceLabel = $('#'+ids[0]+'-price').text(product.prices[ids[1]] + ' Reais')
        for (var i = 0; i < ids[2]+1; i++) {
            if (i == ids[1]) {
                $('#'+ids[0]+'-'+i+'-'+ids[2]).addClass('active');
            } else {
                $('#'+ids[0]+'-'+i+'-'+ids[2]).removeClass('active');
            }
        }
    })

    $('.addcart').click(function() {
        let $this = $(this);
        let ids = this.id.split('-');
        let product = products[ids[1]];
        let price = 0;
        for (var i = 0; i < Number(ids[2])+1; i++) {
            if ($('#'+ids[1]+'-'+i+'-'+ids[2]).hasClass('active')) price = i;
        }

        var cartCount = $('#itensCart');
        var currentVal = parseInt(cartCount.html());
        cartCount.html(currentVal + 1).change();

        var total = $('#total');
        var totalwithfee = $('#totalwithfee');
        var fee = $('#fee');

        var currentValTotal = parseFloat(total.html()) + (product.prices[price]);
        var currentValFee = (5 * currentValTotal) / 100;
        var currentValFeeFee = (5 * currentValFee) / 100;
        currentValFee = currentValFee + currentValFeeFee;
        var currentValTotalWithFee = currentValTotal + currentValFee;

        total.html(currentValTotal.toFixed(2)).change();
        totalwithfee.html(currentValTotalWithFee.toFixed(2)).change();
        fee.html(currentValFee.toFixed(2)).change();

        var productsValue = (product.prices[price]);
        let productsCart = $('#products');
        $('#products').append($(`<div id="${ids[1]}-${productsCart.children().length + 1}-${price}" class="productRow"><img src="${product.url}" style="height: 50px; flex: 1; margin-right: 10px;"><div class="cartProduct"><h5>${product.name}</h5><h6>Valor: R$ ${productsValue.toFixed(2)}</h6></div><a style="color: #f5365c; cursor: pointer; flex: 1;" onclick="removeCart('${ids[1]}-${productsCart.children().length + 1}-${price}')" class="ml-auto"><svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-x-circle"><circle cx="12" cy="12" r="10"></circle><line x1="15" y1="9" x2="9" y2="15"></line><line x1="9" y1="9" x2="15" y2="15"></line></svg></a></div>`));
    })

    function removeCart(id) {
        let array = id.split('-');
        let productID = array[0];
        let product = products[productID];
        let price = array[2];
        let Cartproduct = $(`#${id}`);
        Cartproduct.remove();

        var cartCount = $('#itensCart');
        var currentVal = parseInt(cartCount.html());
        cartCount.html(currentVal - 1).change();

        var total = $('#total');
        var totalwithfee = $('#totalwithfee');
        var fee = $('#fee');
        var currentValTotal = parseFloat(total.html()) - (product.prices[price]);
        var currentValFee = (5 * currentValTotal) / 100;
        var currentValTotalWithFee = currentValTotal + currentValFee;

        total.html(currentValTotal.toFixed(2)).change();
        totalwithfee.html(currentValTotalWithFee.toFixed(2)).change();
        fee.html(currentValFee.toFixed(2)).change();
    }
</script>
<script src="/public/assets/js/app.js"></script>
</body>
</html>